{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": "Blog posts migrated from Contentful"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "blogText": {
      "type": "richtext",
      "required": false
    },
    "contentfulRichText": {
      "type": "json",
      "required": false
    },
    "author": {
      "type": "string",
      "required": false
    },
    "status": {
      "type": "enumeration",
      "default": "draft",
      "enum": [
        "draft",
        "published",
        "archived"
      ]
    },
    "contentType": {
      "type": "string"
    },
    "lastUpdatedBy": {
      "type": "string",
      "required": false
    },
    "publishedDate": {
      "type": "datetime",
      "required": false
    },
    "tags": {
      "type": "json",
      "required": false
    }
  }
}
